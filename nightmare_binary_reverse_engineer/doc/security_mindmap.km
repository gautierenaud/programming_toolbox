                                        "children": []
                                    },
                                    {
                                        "data": {
                                            "id": "cfdvai9vgu80",
                                            "created": 1635711379972,
                                            "text": "Solution: leak an address, then compute the random delta using the original address from libc",
                                            "priority": null,
                                            "progress": null,
                                            "background": "#92d050",
                                            "layout": null
                                        },
                                        "children": []
                                    }
                                ]
                            },
